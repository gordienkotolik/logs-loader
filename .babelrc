{
  "presets": [
    ["es2015", {
      "es2015": {
        "loose": true,
        "modules": false
      }
    }],
    "es2016",
    "es2017",
    "stage-0"
  ],
  "plugins": [
    "add-module-exports",
    ["transform-replace-object-assign", "simple.assign"],
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-object-rest-spread"
  ],
  "only": [
    "src/**/*"
  ]
}